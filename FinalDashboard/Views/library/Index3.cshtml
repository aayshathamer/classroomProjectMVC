@model IEnumerable<FinalDashboard.Models.BLibrary>
@{
    ViewBag.Title = "Index3";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2 class="text text-bg-dark text-center">
    <p class="fw-bold">Books</p>
    <i class="bi bi-person-circle">   </i>

</h2>

<table class="table table-dark table-hover">

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Book)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dep)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Book)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dep)
            </td>


        </tr>
    }

</table>
