@model FinalDashboard.Models.student
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_dashLayout.cshtml";
}

<center><h2>Hello Admin</h2></center>
<!-- Post Content-->
<!-- Main content -->
<div class="container-fluid">
    <!-- Small boxes (Stat box) -->
    <div class="col-lg-3 col-6">
        <!-- small box -->
        <div class="small-box bg-info">
            <div class="inner">
                <h3> @ViewBag.Users </h3>

                <p>Number of Students</p>
            </div>
            <div class="icon">
                <i class="ion ion-person"></i>
            </div>
            <a href="~/Reg/Index" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
</div>
<h2>Mangage Pages</h2>
<br>

<section class="content">
    <div class="container-fluid">
        <!-- Small boxes (Stat box) -->

        <div class="row">
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-info">
                    <div class="inner">
                        <h3>CS</h3>

                        <p>courses</p>
                    </div>

                    <a href="~/CS/Index2" class="small-box-footer">Content<i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-success">
                    <div class="inner">
                        <h3>SWE</h3>

                        <p>courses</p>
                    </div>

                    <a href="~/SWE/Index2" class="small-box-footer">Content <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-warning">
                    <div class="inner">
                        <h3>ICT</h3>

                        <p>courses</p>
                    </div>

                    <a href="~/ICT/Index2" class="small-box-footer">Content <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-warning">
                    <div class="inner">
                        <h3>Courses</h3>

                        <p>available</p>
                    </div>

                    <a href="~/professor/Index2" class="small-box-footer">Content <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-danger">
