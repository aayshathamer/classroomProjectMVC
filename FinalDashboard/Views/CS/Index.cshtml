@model FinalDashboard.Models.C
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Page Header-->
<header class="masthead" style="background-image: url('../../assets/img/istockphoto-515704080-612x612.jpg'); ">
    <div class="container position-relative px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-md-10 col-lg-8 col-xl-7">
                <div class="post-heading">
                    <h1>Computer Science</h1>
                    <h2 class="subheading">stay updated.</h2>
                    <span class="meta">
                        <a href="#!">Class of 2024</a>

                    </span>
                </div>
            </div>
        </div>
    </div>
</header>

<!-- Main content -->
<div class="container-fluid">
    <!-- Small boxes (Stat box) -->
    <div class="col-lg-3 col-6">
        <!-- small box -->
        <div class="small-box bg-info">
            <div class="inner">
                <h3> @ViewBag.courses </h3>

                <p>Number of courses</p>
            </div>
            <a href="~/CS/Index3" class="small-box-footer">Semester Courses <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
</div>
<!-- Post Content-->

<div class="post-heading">
    <h1>Resources:</h1>

</div>
<br>

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-info">
                    <div class="inner">
                        <h3>3<sup style="font-size: 20px">ch</sup></h3>

                        <p>MVC </p>
                    </div>

                    <a href="#" class="small-box-footer">Content<i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-success">
                    <div class="inner">
                        <h3>2<sup style="font-size: 20px">ch</sup></h3>

                        <p>Mobile Computing </p>
                    </div>

                    <a href="#" class="small-box-footer">Content <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-warning">
                    <div class="inner">
                        <h3>5<sup style="font-size: 20px">ch</sup></h3>

                        <p>Research </p>
                    </div>

                    <a href="#" class="small-box-footer">Content <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-danger">
                    <div class="inner">
                        <h3>4<sup style="font-size: 20px">ch</sup></h3>

                        <p>Operating System </p>
                    </div>

                    <a href="#" class="small-box-footer">Content <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
        </div>
    </div>
</section>
<!-- /.row -->
<!-- Footer-->
<footer class="border-top">
    <div class="container px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-md-10 col-lg-8 col-xl-7">
                <ul class="list-inline text-center">
                    <li class="list-inline-item">
                        <a href="#!">
                            <span class="fa-stack fa-lg">
                                <i class="fas fa-circle fa-stack-2x"></i>
                                <i class="fab fa-twitter fa-stack-1x fa-inverse"></i>
                            </span>
                        </a>
                    </li>
                    <li class="list-inline-item">
                        <a href="#!">
                            <span class="fa-stack fa-lg">
                                <i class="fas fa-circle fa-stack-2x"></i>
                                <i class="fab fa-facebook-f fa-stack-1x fa-inverse"></i>
                            </span>
                        </a>
                    </li>
                    <li class="list-inline-item">
                        <a href="#!">
                            <span class="fa-stack fa-lg">
                                <i class="fas fa-circle fa-stack-2x"></i>
                                <i class="fab fa-github fa-stack-1x fa-inverse"></i>
                            </span>
                        </a>
                    </li>
                </ul>
                <div class="small text-center text-muted fst-italic">Copyright &copy; Gollis University</div>
            </div>
        </div>
    </div>
</footer>

